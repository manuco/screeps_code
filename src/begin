function run() {
    var c1 = Game.spawns["Spawn1"].createCreep([MOVE, CARRY, WORK], undefined, {role: "harvester"});
    var c2 = Game.spawns["Spawn1"].createCreep([MOVE, CARRY, WORK], undefined, {role: "harvester"});

    console.log("Nouveaux creeps :", c1, c2);
}

module.exports = {
    begin: run
};
